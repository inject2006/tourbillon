{
  "modules": {
    "auth": {
      "enable": true,
      "api_url": "/misc/verify"
    },
    "login": {
      "api_url": "/misc/login",
      "page": "/_modules/account/login.html",
      "data": [
        {
          "name": "username",
          "type": "text",
          "placeholder": "用户名"
        },
        {
          "name": "password",
          "type": "password",
          "placeholder": "密码"
        }
      ],
      "success_redirect": "/example/table/flight-order-table.html"
    },
    "register": {
      "api_url": "/account/register",
      "page": "/_modules/account/register.html",
      "data": [
        {
          "name": "username",
          "type": "text",
          "placeholder": "用户名"
        },
        {
          "name": "email",
          "type": "email",
          "placeholder": "邮箱"
        },
        {
          "name": "password",
          "type": "password",
          "placeholder": "密码"
        }
      ],
      "success_redirect": "/index.html"
    }
  }
}
